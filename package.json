{
  "name": "anno-rep-frontend",
  "description": "Frontend for the AnnoREP tool to Restructure, Edit, and Publish annotations",
  "author": {
    "name": "QualitativeDataRepository"
  },
  "version": "0.1.0",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/QualitativeDataRepository/AnnoREP-Frontend.git"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max_old_space_size=2096 next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "lint:fix": "npm run lint -- --fix",
    "test:unit": "jest --watch",
    "test:all-unit": "jest",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "npm run format"
    ]
  },
  "dependencies": {
    "@carbon/icons-react": "^10.33.0",
    "@prisma/client": "^2.27.0",
    "@types/carbon-components-react": "^7.34.0",
    "@types/carbon__icons-react": "^10.31.0",
    "axios": "^0.21.1",
    "carbon-components": "^10.37.0",
    "carbon-components-react": "^7.37.1",
    "carbon-icons": "^7.0.7",
    "form-data": "^4.0.0",
    "formidable": "^1.2.2",
    "next": "10.0.7",
    "next-auth": "^3.27.0",
    "qs": "^6.10.1",
    "react": "^16.14.0",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^16.14.0",
    "react-pdf": "^5.3.0",
    "react-sizeme": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addons": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@storybook/theming": "^6.1.21",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.5",
    "@types/formidable": "^1.2.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "^17.0.8",
    "@types/react-pdf": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react-docgen-typescript": "^1.21.0",
    "storybook-css-modules-preset": "^1.1.1",
    "typescript": "^4.2.2"
  }
}
